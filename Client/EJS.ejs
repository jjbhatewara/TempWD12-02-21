<!DOCTYPE html>
<html lang="en">
<head>
    <style>
        h1 {text-align: center;}
        table{margin: 50px auto;}
        table, th, td {
    border-collapse: collapse;
    border: 1px solid #000;
}
    </style>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Display</title>
</head>
<body>
    <h1> Records in mySql Database </h1>
    <table id="table">
        <thead>
            <tr>
            <th>ID</th>
            <th>NAME</th>
            <th>ROLL NUMBER</th>
            <th>BRANCH</th>
            </tr>
        </thead>

        <tbody>
            <% rows.forEach(function(row){ %>
                <tr>
                <th> <%= row.ID %> </th>
                <th> <%= row.Name %> </th>
                <th> <%= row.Roll %> </th>
                <th> <%= row.branch %> </th>
                </tr>
            <% }); %>
        </tbody>

    </table>
    </main>
    <!-- <script src="display.js"  ></script> -->
</body>
</html>
